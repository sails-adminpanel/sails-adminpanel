{
	"bugs": {
		"url": "https://github.com/sails-adminpanel/sails-adminpanel/issues"
	},
	"dependencies": {
		"adminizer": "next"
	},
	"devDependencies": {
		"@42pub/typed-sails": "^1.0.1"
	},
	"deprecated": false,
	"description": "Basic adminpanel for Sails 1x",
	"homepage": "https://github.com/sails-adminpanel/sails-adminpanel#readme",
	"keywords": [
		"sails-hook",
		"sails",
		"sails.js",
		"hook",
		"admin",
		"admin-panel",
		"adminpanel"
	],
	"license": "MIT",
	"main": "index.js",
	"name": "sails-adminpanel",
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/sails-adminpanel/sails-adminpanel.git"
	},
	"sails": {
		"isHook": true,
		"hookName": "adminpanel"
	},
	"scripts": {
		"gulp": "gulp prod",
		"test": "ts-mocha test/bootstrap.ts './test/{,!(fixture)/**}/*.test.ts' --exit",
		"test:js": "mocha test/bootstrap.js './test/{,!(fixture)/**}/*.test.js' --exit",
		"test:init": "npm i && cd ./test/fixture && npm i --no-package-lock && cd -",
		"test:init-win": "npm i && cd test\\fixture && rmdir /s/Q node_modules && npm i --no-package-lock",
		"build:css": "sass assets/styles:clarity/src",
		"bulid:js": "npx gulp js && npx gulp jsProd",
		"tslint": "npx eslint ."
	},
	"version": "3.3.0"
}
