<% var value = record[key] %>
<% if (field.model.type == 'boolean') { %>
    <i class="las la-<%= value ? "check-circle" : "ban" %>"></i>
<% } else if (field.model.type == 'text') { %>
    <%- value %>
<% } else if (field.model.type == 'json') { %>
    <%- JSON.stringify(value) %>
<% } else if (field.model.type == 'association') { %>
    <%- adminpanel.viewHelper.getAssociationValue(value, field) %>
<% } else if (field.model.type == 'association-many') { %>
    <%- adminpanel.viewHelper.getAssociationValue(value, field) %>
<% } else { %>
    <% if (field.config.enum) { %>
        <%- field.config.enum[value] %>
    <% } else { %>
        <%- value %>
    <% } %>
<% } %>
